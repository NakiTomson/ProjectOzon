<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_pressed="true"
        android:state_checked="false"
        >
        <shape android:shape="rectangle"  >
            <corners android:radius="6dp" />
            <stroke android:width="0dip" android:color="#43BC18" />
            <gradient android:angle="180" android:startColor="#43BC18" android:endColor="#43BC18" />
        </shape>

    </item>



    <item android:state_focused="true"

        android:state_checked="false"
        >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <stroke android:width="0dip" android:color="#62EF30" />
            <gradient android:angle="180" android:startColor="#62EF30" android:endColor="#62EF30" />
        </shape>
    </item>


    <item>
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <stroke android:width="0dip" android:color="#39BBF6"/>
            <gradient android:angle="180" android:startColor="#62EF30" android:endColor="#62EF30" />
        </shape>
    </item>



</selector>